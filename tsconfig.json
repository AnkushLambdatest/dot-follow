{
  "compilerOptions": {
    "esModuleInterop": true,
    "declaration": true,
    "importHelpers": true,
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "target": "es2019",
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
    "paths": {
      "commands/*": ["src/commands/*"],
      "lib/*": ["src/lib/*"]
    }
  },
  "include": ["src/**/*"]
}
